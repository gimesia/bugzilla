@model IEnumerable<bugzilla.Models.Developer>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome to Bugzilla</h1>
    <form>
        <select>
            @foreach (var dev in Model)
            {
                <option value="@dev.Name">@dev.Name</option>
            }
        </select>
    </form>
    <form asp-controller="Developer" asp-action="Index">
        <button type="submit">Go</button>
    </form>
</div>